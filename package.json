{
  "name": "mvp-userservice",
  "version": "1.0.0-development.41",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "upgrade": " nest update -t latest",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "standardx \"src/**/*.ts\"",
    "lint:fix": "standardx --fix \"src/**/*.ts\"",
    "test": "jest --config=jest.json --passWithNoTests",
    "test:watch": "jest --watch --config=jest.json",
    "test:cov": "echo 'redundant step, skipping'",
    "test:e2e": "jest --config=jest-e2e.json --verbose --coverage --coverageDirectory=coverage  --forceExit --passWithNoTests",
    "test:e2e:watch": "jest --config=jest-e2e.json --verbose --coverage --coverageDirectory=coverage  --forceExit --passWithNoTests --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/ormconfig-migration.ts",
    "typeorm:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "typeorm:generate": "npm run typeorm migration:generate",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run prebuild && npm run build && npm run typeorm migration:revert",
    "keycloak:scripts": "ts-node ./src/scripts/console",
    "typeorm:seed": "ts-node ./src/seeders/console"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.6",
