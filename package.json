  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "no-useless-constructor": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "no-return-assign": "off",
      "no-unused-expressions": "off",
      "semi": "off",
      "no-return-await": "off",
      "indent": [
        "error",
        4
      ]
    },
    "env": {
      "node": true,
      "jest": true
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
